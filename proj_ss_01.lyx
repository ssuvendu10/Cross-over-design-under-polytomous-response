\end_layout

\begin_layout Plain Layout

    Q[i] = sqrt((((tau.1[i])^2) + ((tau.2[i])^2) - (2*cor_coef[i]*tau.1[i]*tau.2[i])
)/(1 - (cor_coef[i])^2) )
\end_layout

\begin_layout Plain Layout

  }
