\begin_layout Plain Layout

<<eval=F>>=
\end_layout

\begin_layout Plain Layout

g.func<- function(x){
\end_layout

\begin_layout Plain Layout

  func_value = log(x/(1-x))
\end_layout

\begin_layout Plain Layout

  return(func_value)
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

sigma_xy<- function(x,y){
\end_layout

\begin_layout Plain Layout

  sig_xy = 1/((1-x)*y)
\end_layout

\begin_layout Plain Layout

  return(sig_xy)
\end_layout

\begin_layout Plain Layout

}
\end_layout

