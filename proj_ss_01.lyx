\begin_layout Plain Layout

gamma.log<- function(x,a){
\end_layout

\begin_layout Plain Layout

  g = exp(a + log(x))/(1 + exp(a + log(x)))
\end_layout

\begin_layout Plain Layout

  return(g)
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout

