\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=\frac{1}{(1+\alpha)}(\pi_{k0}[\pi_{kk's}+\alpha_{0}\left(\pi_{kk's}-\pi_{k0}+1\right)+\alpha_{1}\left(\pi_{kk's}-\pi_{k1}+0\right)+\cdot\cdot\cdot+\alpha_{G}\left(\pi_{kk's}-\pi_{kG}+0\right)]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
+\pi_{k1}[\pi_{kk's}+\alpha_{0}\left(\pi_{kk's}-\pi_{k0}+0\right)+\alpha_{1}\left(\pi_{kk's}-\pi_{k1}+1\right)+\cdot\cdot\cdot+\alpha_{G}\left(\pi_{kk's}-\pi_{kG}+0\right)]
\]

\end_inset


\end_layout

\begin_layout Standard
