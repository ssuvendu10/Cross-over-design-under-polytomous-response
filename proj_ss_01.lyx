    Q[i] = (tau.2[i] - (cor_coef[i]*tau.1[i]))/(1 - (cor_coef[i])^2)
\end_layout

\begin_layout Plain Layout

  }
\end_layout

\begin_layout Plain Layout

  else if(tau.2[i] < tau.1[i] && tau.2[i] < 0){
\end_layout

\begin_layout Plain Layout

    Q[i] = (tau.1[i] - (cor_coef[i]*tau.2[i]))/(1 - (cor_coef[i])^2)
\end_layout

\begin_layout Plain Layout

  }
\end_layout

\begin_layout Plain Layout

  else{
\end_layout

\begin_layout Plain Layout

    Q[i] = 0
\end_layout

\begin_layout Plain Layout

